{{#each recipes as |recipe|}}
  {{log recipe}}
  <section data-id="{{recipe.id}}">
    <h6>ID:</h6>
    <h3>
      {{recipe.id}}
    </h3>
    <h6>Title:</h6>
    <h3>
      {{recipe.title}}
    </h3>
    <h6>Ingredients:</h6>
    <h3>
      {{recipe.ingredients}}
    </h3>
    <h6>Instructions:</h6>
    <h3>
      {{recipe.instructions}}
    </h3>
    <h6>Notes:</h6>
    <h3>
      {{recipe.notes}}
    </h3>
  </section>

{{/each}}
